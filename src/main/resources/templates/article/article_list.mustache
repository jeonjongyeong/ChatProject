{{>layouts/header}}
<div class="container">
    <table class="table">
        <thead>
        <tr>
            <th scope="col">번호</th>
            <th scope="col">제목</th>
            <th scope="col">내용</th>
        </tr>
        </thead>
        <tbody>
        {{#articles}}
            <tr>
                <th>{{id}}</th>
                <td><a href="/article/detail/{{id}}">{{title}}</a></td>
                <td>{{body}}</td>
            </tr>
        {{/articles}}
        </tbody>
    </table>
    <a href="/article/new" class="btn btn-primary">생성하기</a>
</div>
{{>layouts/footer}}




